{
    "name": "nadar/page-crawler",
    "type": "library",
    "authors": [
        {
            "name": "Basil Suter",
            "email": "git@nadar.io"
        }
    ],
    "require": {
        "ext-curl" : "*"
    },
    "autoload": {
        "psr-4": {
            "Nadar\\PageCrawler\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nadar\\PageCrawler\\Tests\\" : "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "smalot/pdfparser": "^0.16.1"
    },
    "suggest": {
        "smalot/pdfparser" : "In order to use the PdfParser this library is required."
    }
}
